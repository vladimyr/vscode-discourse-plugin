{
  "scopeName": "html-discourse.injection",
  "injectionSelector": "text.html",
  "patterns": [
    {
      "include": "#discourse-plugin"
    }
  ],
  "repository": {
    "discourse-plugin": {
      "begin": "(?<=<script type=\"text/discourse-plugin\" version=\"0.8.20\">)",
      "end": "(?=</script>)",
      "contentName": "meta.embedded.block.html.discourse",
      "patterns": [
        {
          "include": "source.js"
        }
      ]
    }
  }
}